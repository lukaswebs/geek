
    <div class="section first" id="section0">
        {% if content.field_logo %}
            <img src="{{ file_url(node.field_logo.entity.fileuri) }}" alt="logo" />
        {% endif %}
        {% if content.field_paragraph_text %}
		<div class="contextual">
			<p>{{ content.field_paragraph_text[0] }}</p>
		</div>
        {% endif %}
		<a href="#" id="moveSectionDown" class="scroll-down moveSectionDown"></a>
	</div>
        
    <div class="section illustrated" id="section1">
        {% if content.field_section_image %}
		<img src="{{ file_url(node.field_section_image.entity.fileuri) }}" alt="first" />
        {% endif %}
		<div class="contextual">
			<h4>
                {% if content.field_h4_dark_color_start %}
                    {{ content.field_h4_dark_color_start[0] }}
                {% endif %}
                {% if content.field_h4_red_color %}
                    <span>{{ content.field_h4_red_color[0] }}</span>
                {% endif %}
                {% if content.field_h4_dark_color_end %}
                    {{ content.field_h4_dark_color_end[0] }}
                {% endif %}
            </h4>
		</div>
		<a href="#"  class="scroll-down moveSectionDown"></a>
	</div>
            
